function cats(input) {

    let demo = [];
    class Cat {
        constructor(name, age) {
            this.name = name;
            this.age = age;
        }
    }

    function meow() {
        console.log(`${demo.name}, age ${demo.age} says Meow`);
    }
    for (each of input) {
        let a = each.split(" ");
        let demo = new Cat(a[0], a[1]);
        meow();
    }

}
